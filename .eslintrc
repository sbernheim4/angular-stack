{
	"parserOptions": {
		"ecmaVersion": 6
	},

	"globals": {
		"app": true,
		"angular": true,
		"$scope": true,
		"scope": true,
		"$state": true,
		"require": true,
		"process": true,
		"__dirname": true,
		"console": true
	},

	"rules": {
		"no-console": 1,
		"no-debugger": 2,
		"no-extra-parens": 1,
		"no-irregular-whitespace": 2,
		"no-unreachable": 2,
		"use-isnan": 1,
		"valid-jsdoc": 1,
		"eqeqeq": 2,
		"no-alert": 2,
		"no-multi-spaces": 1,
		"no-native-reassign": 1,
		"no-octal": 2,
		"strict": 0,
		"no-undef": 2,
		"no-undef-init": 1,
		"no-undefined": 2,
		"no-unused-vars": 1,
		"camelcase": 2,
		"consistent-this": 1,
		"eol-last": 1,
		"no-nested-ternary": 0,
		"no-trailing-spaces": 2,
		"quotes": 0,
		"space-infix-ops": 1,
		"no-var": 1,
	}
}
